## -*- coding: utf-8 -*-
<%inherit file="base.tmpl"/>
<%block name="content">
        <!--Body content-->
        <div class="post">
         <div class="page-header dashed">
                    <h1 class="h2">${title}</h1>
        </div>
        <ul class="unstyled">
        % for text, link in items:
            <li><a href="${link}">${text}</a>
        % endfor
        </ul>
        </div>
        <!--End of body content-->
</%block>
